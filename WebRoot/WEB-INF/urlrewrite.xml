<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE urlrewrite PUBLIC "-//tuckey.org//DTD UrlRewrite 2.6//EN"
        "http://tuckey.org/res/dtds/urlrewrite2.6.dtd">
<urlrewrite>
	<rule>
		<note>商品详情</note>
		<from>^/goods_([0-9]+).htm$</from>
		<to type="forward">/goods.htm?id=$1</to>
	</rule>
	<rule>
		<note>商品立即购买</note>
		<from>^/good_buy_([0-9]+).htm$</from>
		<to type="forward">/good_buy.htm?id=$1</to>
	</rule>
	<rule>
		<note>样品采购</note>
		<from>^/sample_buy_([0-9]+).htm$</from>
		<to type="forward">/sample_buy.htm?id=$1</to>
	</rule>
	<rule>
		<note>加入采购单</note>
		<from>^/add_goodscart_([0-9]+).htm$</from>
		<to type="forward">/add_goodscart.htm?id=$1</to>
	</rule>
</urlrewrite>

